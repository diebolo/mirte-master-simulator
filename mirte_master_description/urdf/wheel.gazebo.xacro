<?xml version="1.0"?>
<robot xmlns:xacro="http://ros.org/wiki/xacro">
    <xacro:macro name="wheel_gazebo" params="id">
        <gazebo reference="wheel_${id}"> <!-- ${tf_namespace}-->
            <selfCollide>false</selfCollide>
            <gravity>true</gravity>
            <mu1>0.0</mu1>
            <mu2>0.0</mu2>
            <material>Gazebo/DarkGrey</material>
        </gazebo>
    </xacro:macro>
</robot>